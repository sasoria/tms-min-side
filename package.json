{
  "name": "tms-min-side",
  "version": "1.0.0",
  "description": "min-side",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build --base=https://cdn.nav.no/min-side/tms-min-side/ --outDir dist",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint --fix --ext .js,.jsx .",
    "lint:staged": "eslint -c .eslintrc.pre-commit.cjs --fix --ext .js,.jsx ."
  },
  "author": "navikt",
  "license": "MIT",
  "dependencies": {
    "@nanostores/react": "0.5.0",
    "@navikt/ds-react": "2.9.3",
    "@navikt/ds-tokens": "2.9.3",
    "@navikt/nav-dekoratoren-moduler": "1.9.0",
    "@sentry/react": "7.51.2",
    "react-router": "6.11.0",
    "react-router-dom": "6.11.0",
    "swr": "2.1.5"
  },
  "devDependencies": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "@types/react-modal": "3.16.0",
    "@vitejs/plugin-react": "4.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-import-map": "3.0.0",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "5.0.4",
    "vite": "4.3.4",
    "vite-plugin-mock": "2.9.6"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint -c .eslintrc.pre-commit.cjs --fix",
      "prettier --cache --write",
      "git add"
    ],
    "*.{css,md,html,json}": [
      "prettier --cache --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=18.15.0"
  }
